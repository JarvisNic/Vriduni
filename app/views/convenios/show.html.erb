<% @nav = "convenio" %>
<div style="width:100%;">
<table class="table table-bordered table-hover">
  <tr>
    <td style="width:22%"><strong style="float:right;">Nombre    ></strong></td>
    <td><%= @convenio.nombre %></td>
</tr>

  <tr>
    <td style="width:22%"><strong style="float:right;">Objetivo    ></strong></td>
    <td><%= @convenio.objetivo %></td>
</tr>

  <tr>
    <td style="width:22%"><strong style="float:right;">Responsable    ></strong></td>
    <td><%= @convenio.responsable %></td>
</tr>

  <tr>
    <td style="width:22%"><strong style="float:right;">Área    ></strong></td>
    <td><%= @convenio.area %></td>
</tr>

  <tr>
    <td style="width:22%"><strong style="float:right;">Pdf    ></strong></td>
    <td><%= link_to @convenio.pdf,"http://webvrid.uni.edu.ni/documents/"+@convenio.pdf,:target => "_blank" %></td>
</tr>

  <tr>
    <td style="width:22%"><strong style="float:right;">Categoría    ></strong></td>
    <td><%= @convenio.categoria %></td>
</tr>

  <tr>
    <td style="width:22%"><strong style="float:right;">Fecha inicio    ></strong></td>
    <td><%= @convenio.fecha_inicio %></td>
</tr>

  <tr>
    <td style="width:22%"><strong style="float:right;">Fecha fin    ></strong></td>
    <td><%= @convenio.fecha_fin %></td>
</tr>

  <tr>
    <td style="width:22%"><strong style="float:right;">Fecha firma    ></strong></td>
    <td><%= @convenio.fecha_firma %></td>
</tr>
</table>
<% if current_user and current_user.tipo.to_i == 2 %>
<%= link_to 'Editar', edit_convenio_path(@convenio)  ,class: "btn btn-default" %> |
<% end %>
<a href="javascript:history.back()" class="btn btn-primary btn-lg" >Atrás</a>

